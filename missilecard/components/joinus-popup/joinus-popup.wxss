/* components/joinus-popup/joinus-popup.wxss */
@import '../../styles/ztip.wxss';
/*弹窗*/
.picker{
  position: relative;
  height: 72rpx;
  padding-left: 30rpx;
  color: #333333;
  font-size: 28rpx;
  line-height: 72rpx;
  border-radius: 12rpx;
  border: 1rpx solid #cdcdcd;
  background-color: #f5f5f5;
}
.picker-text{
  font-size: 28rpx;
  color: #333333;
  display: inline-block;
  width: 560rpx;
}
.zt-flex{
  display: flex;
  justify-content: space-between;
  align-items: center;
}