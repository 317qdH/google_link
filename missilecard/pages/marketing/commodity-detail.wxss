/* pages/marketing/commodity-detail.wxss */
.commodity-detail-swiper{
  height: 750rpx;
}
.commodity-detail-swiper .slide-image{
  width: 750rpx;
  height: 750rpx;
}
.commodity-detail{
  padding: 30rpx 0 0 0;
}
.commodity-detail .cd-title{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
  color: #111111;
  font-size: 32rpx;
  line-height: 46rpx;
  margin: 0 30rpx 10rpx 30rpx;
}
.commodity-detail .cd-remark{
  color: #999999;
  font-size: 26rpx;
  margin: 0 30rpx 0 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.commodity-detail .cd-price{
  color: #d94e43;
  font-size: 32rpx;
  padding: 0 30rpx 30rpx 30rpx;
  border-bottom: 1rpx solid #eeeeee;
}
.commodity-detail .cd-price>text{
  font-size: 46rpx;
}
.cd-price-region{
  display: flex;
  margin: 30rpx 0 ;
  padding: 0 30rpx;
}
.cd-region-item{
  display: flex;
  flex-direction: column;
  text-align: left;
  margin-right: 50rpx;
}
.cd-region-item .cdr-number{
  color: #999999;
  font-size: 26rpx;
}
.cd-region-item .cd-remark{
  color: #999999;
  font-size: 26rpx;
  margin: 0 30rpx 30rpx 30rpx;
}
.cd-region-item .cdr-price{
  color: #d94e43;
  font-size: 32rpx;
  margin-left: -10rpx;
}
.cd-region-item .cdr-price>text{
  font-size: 34rpx;
}
.recommond-cd-tip{
  background-color: #f9f9f9;
}

.cd-tip{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 16rpx 60rpx;
  border-bottom: 20rpx solid #f0f0f0;
}
.cd-tip-item{
  line-height: 42rpx;
  text-align: center;
}
.cd-tip>view{
  color: #999999;
  font-size: 26rpx;
}
.cd-tip>text{
  color: #333333;
  font-size: 26rpx;
}
.model-number{
  position: relative;
  padding: 32rpx;
  color:#333333;
  font-size: 32rpx;
  border-bottom: 20rpx solid #f0f0f0;
  overflow: visible;
}
.model-number::after{
  content: '';
  width: 20rpx;
  height: 20rpx;
  border-right: 2rpx solid #999999;
  border-bottom: 2rpx solid #999999;
  transform: rotate(-45deg);
  position: absolute;
  right: 32rpx;
  top: 42rpx;
}
/*公司详情*/
.cd-company{
  padding: 20rpx;
}
.cd-company-bg{
  display: flex;
  align-items: center;
  border-radius: 20rpx;
  background: linear-gradient(to right, #65a5ff, #1f7dff 50%);
}
.cd-company-bg>image{
  width: 100rpx;
  height: 100rpx;
  padding: 40rpx 20rpx 40rpx 30rpx;
  border-radius: 4rpx;
}
.cd-company-detail{
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.cd-company-name{
  width: 520rpx;
  color: #fff;
  font-size: 34rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.cd-company-infor{
  display: flex;
  align-items: center;
  color: #fff;
  font-size: 28rpx;
}
.cd-company-check{
  width: 52rpx;
  position: relative;
  margin-left: 40rpx;
  font-size: 26rpx;
  color: #8abbff;
  overflow: visible;
}
.cd-company-check::after{
  content: '';
  width: 12rpx;
  height: 12rpx;
  border-right: 2rpx solid #8abbff;
  border-bottom: 2rpx solid #8abbff;
  transform: rotate(-45deg);
  position: absolute;
  right: -20rpx;
  top: 12rpx;
}
.cd-contact-name{
  width: 150rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-left: 46rpx;
  margin-right: 25rpx;
  background: url('http://img0.912688.com/mc-m-business-icons.png') no-repeat;
  background-size: 34rpx 96rpx;
  background-position: 0 8rpx;
}
.cd-contact-iphone{
  padding-left: 40rpx;
  background: url('http://img0.912688.com/mc-m-business-icons.png') no-repeat;
  background-size: 34rpx 96rpx;
  background-position: 0 -61rpx;
}



.cd-product{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 30rpx 0 30rpx;
  overflow: visible;
}
.cd-product-title{
  position: relative;
  color: #333333;
  font-size: 32rpx;
  overflow: visible;
}
.cd-product-title::after{
  content: '';
  width: 126rpx;
  height: 5rpx;
  background-color: #e5e5e5;
  border-radius: 2.5rpx;
  position: absolute;
  left: 0;
  top: 55rpx;
}
.cd-more{
  position: relative;
  padding-right: 20rpx;
  color: #66a6ff;
  font-size: 26rpx;
  overflow: visible;
}
.cd-more::after{
  content: '';
  width: 14rpx;
  height: 14rpx;
  border-right: 3rpx solid #66a6ff;
  border-bottom: 3rpx solid #66a6ff;
  transform: rotate(-45deg);
  position: absolute;
  left: 56rpx;
  top: 11rpx;
}
.cd-product-detail{
  display: flex;
  flex-direction: column;
  padding: 24rpx 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.cd-product-item{
  display: flex;
}
.cd-product-detail .cdpd-left{
  color: #333333;
  font-size: 28rpx;
  width: 240rpx;
  line-height: 54rpx;
}
.cd-product-detail .cdpd-right{
  color: #777777;
  font-size: 28rpx;
  line-height: 54rpx;
}
.cdp-show{
  padding: 50rpx 30rpx 120rpx 30rpx;
}

.mcs-btn-box{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 690rpx;
  height: 120rpx;
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
}
.mcs-btn-box>view{
  width: 330rpx;
  height: 70rpx;
  border-radius: 35rpx;
  line-height: 70rpx;
}
.mcs-btn-box>button{
  width: 330rpx;
  height: 70rpx;
  border-radius: 35rpx;
  line-height: 70rpx;
}
/*查看商品详情*/
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, .5)
}
.Industry-picker{
  position: fixed;
  left: 0;
  bottom: -1100rpx;
  width: 100%;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  background: #fff;
  z-index: 20;
  -webkit-transition:all 1s linear;
  transition:all .3s linear;
}
.Industry-picker-show{
  bottom: 0;
}
.Industry-picker .i-pick-title{
  width: 100%;
  height: 100rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #66a6ff;
  font-size: 16px;
}
.Industry-picker .i-pick-title image{
  width: 22rpx;
  height: 22rpx;
  padding: 20rpx;
  position: absolute;
  right:10rpx;
  top: 18rpx; 
}
.i-pick-btn{
  height:148rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.i-pick-main{
  /* overflow-x: hidden;
  overflow-y: scroll; */
  overflow: scroll;
  height: 850rpx;
  border-top: 1rpx solid #e5e5e5;
  border-bottom: 1rpx solid #e5e5e5;
}
.i-pick-main::-webkit-scrollbar {
  display: none;
}
.product-param-item{
  display: flex;
  padding: 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.cdp-detail-left{
  width: 250rpx;
  color: #777777;
  font-size: 28rpx;
}
.cdp-detail-right{
  color: #333333;
  font-size: 28rpx;
}
/*产品规格悬浮框*/
.cm-product-quantity{
  position: fixed;
  left: 0;
  bottom: -1170rpx;
  width: 100%;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  background: #fff;
  z-index: 20;
  -webkit-transition:all 1s linear;
  transition:all .3s linear;
  overflow: visible;
}
.cm-product-quantity-show{
  bottom: 0;
}
.cmpq-top{
  display: flex;
  overflow: visible;
}
.cmpq-top .cmpq-top-img{
  width: 200rpx;
  height: 200rpx;
  margin: -30rpx 0 30rpx 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  border-radius: 8rpx;
  box-shadow: 0 0 10rpx 5rpx #cccccc;
}
.cmpq-top .cmpq-top-img>image{
  width: 188rpx;
  height: 188rpx;
}
.cmpq-top .cmpq-top-title{
  display: flex;
  flex-direction: column;
  width: 440rpx;
  margin: 20rpx 0 0 20rpx;
}
.cmpq-top .cmpq-top-title>view{
  height: 92rpx;
  color: #111111;
  font-size: 30rpx;
  line-height: 46rpx;
  margin-bottom: 14rpx;
  overflow: hidden;
}
.cmpq-top .cmpq-top-title>text{
  color: #d94e43;
  font-size: 30rpx;
}
.cmpq-top>image{
  width: 22rpx;
  height: 22rpx;
  margin: 22rpx 0 0 6rpx;
  padding: 20rpx;
}
.cmpq-title{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 100rpx 30rpx 110rpx;
  color: #111111;
  font-size: 30rpx;
  background-color: #eeeeee;
}
.cmpq-title>text{
  display: block;
  text-align: center;
  width: 200rpx;
}
.cmpq-size-item{
  display: flex;
  justify-content: space-between;
  width: 550rpx;
  /* height: 127rpx; */
  padding: 26rpx 50rpx 26rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eaeaea;
}
.cmpq-size-item:nth-last-child(1){
  border: none;
}
.cmpq-size-item>text{
  width: 200rpx;
  text-align: center;
  color: #555555;
  font-size: 28rpx;
}
.cmpq-size-item>view{
  width: 200rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
}
.cmpq-size-item>view>text{
  color: #d94e43;
  font-size: 28rpx;
  text-align: center;
}
.cmpq-size-item>view>view{
  margin-top: 6rpx;
  color: #999999;
  font-size: 24rpx;
}
.noRange .cmpq-size-item{
  width: 750rpx;
  padding: 26rpx 110rpx 26rpx 110rpx;
}
.cmpq-btn{
  height: 150rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
/*有区间的产品规格*/
.cmpq-title-add{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 70rpx;
  color: #111111;
  font-size: 30rpx;
  background-color: #eeeeee;
}
.cmpq-color-box{
  display: flex;
  height: 580rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.cmpq-color{
  width: 200rpx;
  height: 580rpx;
}
.cmpq-color-item{
  width: 200rpx;
  height: 76rpx;
  padding: 26rpx 0;
  text-align: center;
  color: #555555;
