page {
  background: #f0f0f0;
}
.header {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  padding-bottom: 30rpx;
  /* height: 582rpx; */
  font-size: 28rpx;
  color: #777777;
  background: #fff;
  margin-bottom: 20rpx;
}
.header text {
  width: 685rpx;
}
.header text:last-child {
  margin-top: 20rpx;
}
.images {
  width: 130rpx;
  height: 130rpx;
  background: #f0f0f0;
  border-radius: 20rpx;
  margin-top: 100rpx;
  margin-bottom: 50rpx;
}
.images image {
  width: 100%;
  height: 100%;
}
.footer {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 540rpx;
  font-size: 28rpx;
  color: #777777;
  background: #fff;
}
.footer text:nth-child(1) {
  margin-top: 40rpx;
  margin-bottom: 36rpx;
  font-size: 32rpx;
  color: #66a7fe;
}
.footer text:nth-child(2) {
  margin-bottom: 50rpx;
}
.footer view {
  width: 105rpx;
  height: 105rpx;
  border-radius: 50%;
  margin-bottom: 25rpx;
}
.footer text:nth-child(3) {
  margin-bottom: 15rpx;
}
.about {
  width: 100%;
  height: 100%;
}